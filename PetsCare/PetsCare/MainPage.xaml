<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:PetsCare"
             x:Class="PetsCare.MainPage">

    <NavigationPage Title="Week Events" Icon="">
        <x:Arguments>
            <local:WeekPage/>
        </x:Arguments>
    </NavigationPage>
    
    <NavigationPage Title="Calendar" Icon="">
        <x:Arguments>
            <local:CalendarPage/>
        </x:Arguments>
    </NavigationPage>
    
    <NavigationPage Title="Pets" Icon="">
        <x:Arguments>
            <local:PetsPage />
        </x:Arguments>
    </NavigationPage>

    <NavigationPage Title="Clinics" Icon="">
        <x:Arguments>
            <local:ClinicsPage/>
        </x:Arguments>
    </NavigationPage>

</TabbedPage>
